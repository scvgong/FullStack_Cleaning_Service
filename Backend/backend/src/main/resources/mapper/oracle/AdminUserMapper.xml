<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cleaning.backend.mapper.AdminUserMapper">
    <select id="findByUsername" resultType="com.cleaning.backend.model.AdminUser">
        SELECT *
        FROM ADMIN_USER
        WHERE USERNAME = #{username}
    </select>
</mapper>